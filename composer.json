{
    "name": "cisco/framework",
    "description": "Création d'un framework fait maison à but d'apprentissage de la programmation orientée objet en php",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "authors": [
        {
            "name": "mohamed",
            "email": "adeboss_3@hotmail.fr"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "vlucas/phpdotenv": "5.4.x-dev",
        "php-di/php-di": "dev-master",
        "symfony/http-foundation": "5.4.x-dev",
        "twig/twig": "3.x-dev"
    },
    "require-dev": {
        "symfony/var-dumper": "5.4.x-dev"
    },
    "scripts": {
        "serve": [
            "php -S localhost:8000 -d display_errors=1 -t public"
        ],
        "cache:clear" : [
        "rd var \\cache\\dev\\twig /S/Q"
        ]
    },
    "config":{
        "process-timeout": 0,
        "optimize-autoloader": true
    }



}
